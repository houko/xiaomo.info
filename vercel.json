{
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/server/.*",
      "headers": {
        "cache-control": "s-maxage=1000"
      },
      "dest": "/server/app.js"
    }
  ]
}
