{
  "name": "xiaomo-server",
  "alias": "xiaomo.server",
  "builds": [
    { "src": "public/**/*", "use": "@now/static" },
    { "src": "api/**/*.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/", "dest": "routes/index.js" },
    { "src": "/users", "dest": "routes/users.js" }
  ],
  "version": 2
}